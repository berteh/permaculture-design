#:kivy 2.1.0
#:import LeftPanel libs.left_panel
#:import CenterDisplay libs.center_display

<Check@MDCheckbox>:
	group: 'group'
	size_hint: None, None
	size: dp(48), dp(48)
	allow_no_selection: False
	radio_icon_down: 'radiobox-marked'

<MetricCheck@MDCheckbox>:
	group: 'group'
	size_hint: None, None
	size: dp(48), dp(48)
	allow_no_selection: False
	radio_icon_down: 'radiobox-marked'

MDBoxLayout:
	LeftPanel:
		id: left_panel
	CenterDisplay:
		id: main_display
